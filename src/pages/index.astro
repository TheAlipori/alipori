---
import Aboutme from "../components/Aboutme.astro";
import Hero from "../components/Hero.astro";
import Navbar from "../components/Navbar.astro";
import Contact from "../components/Contact.astro";
import Layout from "../layouts/Layout.astro";
import Skills from "../components/Skills.astro";
import Footer from "../components/Footer.astro";
import Services from "../components/Services.astro";
import Slider from "../components/Slider.astro";
import Projects from "../components/Projects.astro";
import { getLangFromUrl, useTranslations } from "../i18n/utils";
import SelectLang from "../components/SelectLang.astro";
const lang = getLangFromUrl(Astro.url);
const t = useTranslations(lang);
const titleString = t("title");
const descriptionString = t("description");
import { defaultLang } from "../i18n/ui";
import Cards from "../components/Cards.astro";
import Whyme from "../components/Whyme.astro";
import SectionProjects from "../components/SectionProjects.astro";
import Testimonials from "../components/Testimonials.astro";
import CallToAction from "../components/CallToAction.astro";
Astro.redirect(`/${defaultLang}/`);
---

<Layout title="Pori Labs">
  <Hero />
  <Cards />

  <Aboutme />

  <Whyme />

  <SectionProjects />
